{
  "ignorePatterns": [
  ],
  "replacementPatterns": [
    {
      "pattern": "^/(.*)",
      "replacement": "file:///github/workspace/content/en/$1.md"
    }
  ],
  "httpHeaders": [
  ],
  "timeout": "20s",
  "retryOn429": true,
  "retryCount": 5,
  "fallbackRetryDelay": "30s",
  "aliveStatusCodes": [200, 206]
}
